<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/back_grounds_budget_foods_orders"
    tools:context=".cart">

    <androidx.cardview.widget.CardView
        android:layout_width="411dp"
        android:layout_height="421dp"
        android:background="@drawable/back_g_02"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="96dp" >

        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/recyclerview11"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
    </androidx.cardview.widget.CardView>

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/montserrat_bold"
        android:text="Orders"
        android:textColor="#FFF0E3"
        android:textColorHighlight="#FFF0E3"
        android:textSize="45sp"
        tools:layout_editor_absoluteX="150dp"
        tools:layout_editor_absoluteY="7dp" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="72dp"
        android:layout_height="67dp"
        app:srcCompat="@drawable/cart"
        tools:layout_editor_absoluteX="67dp"
        tools:layout_editor_absoluteY="13dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#F7931E"
        android:text="Button"
        app:backgroundTint="#F7931E"
        tools:layout_editor_absoluteX="159dp"
        tools:layout_editor_absoluteY="643dp" />
</androidx.constraintlayout.widget.ConstraintLayout>